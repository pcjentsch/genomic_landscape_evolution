  file     = {/home/workaccount/Zotero/storage/DVMSIPD9/Volz et al. - 2009 - Phylodynamics of Infectious Disease Epidemics.pdf}
}

@article{volzViralPhylodynamics2013,
  title     = {Viral {{Phylodynamics}}},
  author    = {Volz, Erik M. and Koelle, Katia and Bedford, Trevor},
  year      = {2013},
  month     = mar,
  journal   = {PLOS Computational Biology},
  volume    = {9},
