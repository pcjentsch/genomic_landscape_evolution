@techreport{andersonEstimatingImpactCOVID192020,
  type        = {Preprint},
  title       = {Estimating the Impact of {{COVID-19}} Control Measures Using a {{Bayesian}} Model of Physical Distancing},
  author      = {Anderson, Sean C. and Edwards, Andrew M. and Yerlanov, Madi and Mulberry, Nicola and Stockdale, Jessica E. and Iyaniwura, Sarafa A. and Falcao, Rebeca C. and Otterstatter, Michael C. and Irvine, Michael A. and Janjua, Naveed Z. and Coombs, Daniel and Colijn, Caroline},
  year        = {2020},
